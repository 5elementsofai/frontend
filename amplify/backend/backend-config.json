{
  "hosting": {
    "amplifyhosting": {
      "service": "amplifyhosting",
      "type": "cicd"
    }
  },
  "storage": {},
  "function": {
    "amplify": {
      "providerPlugin": "awscloudformation",
      "service": "LambdaLayer",
      "build": true
    },
    "helloWorld": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": []
    }
  },
  "auth": {},
  "api": {
    "ProductAPI": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 7,
              "description": "Product api key"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  }
}